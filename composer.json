{
    "name": "albinvar/cyph",
    "description": "This is my package cyph",
    "keywords": [
        "albinvar",
        "cyph"
    ],
    "homepage": "https://github.com/albinvar/cyph",
    "license": "MIT",
    "authors": [
        {
            "name": "Albin Varghese",
            "email": "albin8285@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "Cyph\\Cyph\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cyph\\Cyph\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage",
        "test-dev": "vendor/bin/phpunit --testdox --colors=always",
        "format": "vendor/bin/php-cs-fixer fix src --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
